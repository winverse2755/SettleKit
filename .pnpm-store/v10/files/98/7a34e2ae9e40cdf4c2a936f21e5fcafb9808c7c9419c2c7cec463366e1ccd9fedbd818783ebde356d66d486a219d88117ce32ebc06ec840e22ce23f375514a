import type { Provider } from "ethers";
import { type Address, type MarketId, VaultMarketAllocation, type VaultMarketConfig } from "@morpho-org/blue-sdk";
import type { FetchOptions } from "../types";
export declare function fetchVaultMarketAllocation(vault: Address, marketId: MarketId, runner: {
    provider: Provider;
}, options?: FetchOptions): Promise<VaultMarketAllocation>;
export declare function fetchVaultMarketAllocationFromConfig(config: VaultMarketConfig, marketId: MarketId, runner: {
    provider: Provider;
}, options?: FetchOptions): Promise<VaultMarketAllocation>;
